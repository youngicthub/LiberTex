        <script>
            setInterval(() => {

                let allIds = document.querySelectorAll('.randomId')
                for (i = 0; i < allIds.length; i++) {
                    allIds[i].textContent = Math.random()
                    allIds[i].textContent = allIds[i].textContent.substr(3, 6)
                }

                let randomSuccessText = document.querySelectorAll('.successText')
                for (i = 0; i < randomSuccessText.length; i++) {
                    let randomid = Math.floor(Math.random() * 10)
                    randomSuccessText[i].textContent = randomid
                    if (parseInt(randomSuccessText[i].textContent) < 5) {
                        randomSuccessText[i].textContent = 'pending'
                        randomSuccessText[i].style.color = 'red'
                    } else {
                        randomSuccessText[i].textContent = 'success'
                        randomSuccessText[i].style.color = 'green'
                    }
                }


                let randomCoin = document.querySelectorAll('#randomCoin')
                for (i = 0; i < randomCoin.length; i++) {
                    let randomid = Math.random().toString().substr(2, 3)
                    let checkCounName = Math.floor(Math.random() * 10)
                    if (checkCounName < 4) {
                        randomCoin[i].textContent = '0.00' + randomid + ' BTC'
                    } else if (checkCounName > 4 && checkCounName <= 8) {
                        randomCoin[i].textContent = '0.00' + randomid + ' XRP'
                    } else if (checkCounName > 8) {
                        randomCoin[i].textContent = '0.00' + randomid + ' ETH'
                    }
                    //BTC XRP NEO DASH ETH LTC
                }

                let randomTime = document.querySelectorAll('#randomTime')
                for (i = 0; i < randomTime.length; i++) {
                    let minutes = Math.floor(Math.random() * 40)
                    if (minutes < 10) {
                        minutes = '0' + minutes
                    }

                    let hours = new Date().getHours()
                    var ampm = (hours >= 12) ? "pm" : "am";
                    console.log(ampm)

                    randomTime[i].textContent = Math.floor(Math.random() * 11) + 1 + '.' + minutes + ' ' + ampm
                }


            }, 3000)
        </script>


        
